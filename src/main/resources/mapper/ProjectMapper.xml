  <select id="findProjectByStuNo" parameterType="java.lang.String" resultMap="BaseResultMap">
    select p.* from project_select p_s inner join project p
    on p_s.projectno = p.`no` and p_s.company_name = p.company_username
    where
      studentNo = #{_parameter,jdbcType=VARCHAR}
    and p_s.score is null
  </select>
  <!--可以选择的项目 -->
  <select id="selectProjectByStu" parameterType="java.lang.String" resultMap="BaseResultMap">
    SELECT p.* FROM project p
    INNER JOIN student s on p.grade=s.grade
    and p.major = s.professional
    where s.`No`=#{No,jdbcType=VARCHAR};
  </select>
