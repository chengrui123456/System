  <select id="findAllProjectByThree" parameterType="com.entity.Project" resultMap="BaseResultMap">
    SELECT p.* FROM student s
    LEFT JOIN project p on p.grade=s.grade
    and p.major = s.professional
    LEFT JOIN project_category p_c
    on p.category=p_c.category
    where  s.`No`=#{stu_no,jdbcType=VARCHAR}
    <if test="name != null and name != ''">
